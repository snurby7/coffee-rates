{
  "name": "coffee-rates",
  "version": "1.0.0",
  "description": "Application for rating coffee",
  "main": "start.js",
  "scripts": {
    "start": "yarn install --only=prod && node start.js prod",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "server": "tsc-watch --onSuccess \"node start.js dev\" --onFailure \"echo Beep! Compilation Failed\" ",
    "client": "cd ./client && yarn start",
    "test:server": "tsc && node start.js test",
    "build": "sh ./util/buildForProd.sh"
  },
  "dependencies": {
    "@overnightjs/core": "1.3.10",
    "express": "4.16.4",
    "mongoose": "5.4.13"
  },
  "peerDependencies": {
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "prettier": ">=1.13.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.0",
    "@types/express": "4.16.1",
    "@types/jasmine": "3.3.8",
    "@types/mongoose": "5.3.17",
    "@types/node": "10.12.21",
    "@types/supertest": "2.0.7",
    "concurrently": "^4.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx": "^0.0.2",
    "eslint": "5.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jasmine": "3.3.1",
    "nodemon": "^1.18.9",
    "supertest": "3.4.2",
    "tsc": "1.20150623.0",
    "tsc-watch": "1.1.36",
    "tslint": "5.12.1",
    "typescript": "3.3.1",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.1",
    "webpack-node-externals": "1.7.2"
  }
}
